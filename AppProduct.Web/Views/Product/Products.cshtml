@{
    @model List<AppProduct.Models.ProductModel>
}
<table style="width:100%">
    <tr style="height:32px;background-color:aquamarine;border-top:1px solid black;border-bottom:1px solid black">
        <td style="width:20%;border-right:1px solid black">Id</td>
        <td style="width:80%">Название продукта</td>
    </tr>
    @foreach (var product in Model)
    {
            <tr style="height:32px;background-color:antiquewhite;border-bottom:1px solid black">
                <td style="border-right:1px solid black">@product.Id</td>
                <td>@product.Name</td>
            </tr>
    }
</table>
